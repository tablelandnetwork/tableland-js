{
  "name": "@textile/tableland-cli",
  "version": "0.0.1",
  "description": "Tableland experimental command line tool",
  "main": "./dist/cli.js",
  "files": [
    "dist/**/*.js"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/textileio/js-tableland-cli.git"
  },
  "scripts": {
    "build": "npx tsc && chmod +x dist/cli.js",
    "package": "pkg . --targets node17-macos-x64 --output tableland"
  },
  "bin": {
    "tableland": "./dist/cli.js"
  },
  "pkg": {
    "scripts": "./dist/**/*.js"
  },
  "keywords": [
    "cli",
    "tableland",
    "sql",
    "ethereum"
  ],
  "author": "Carson Farmer <carson@textile.io>",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^17.0.17",
    "@types/node-fetch": "^2.5.12",
    "@types/yargs": "^17.0.8",
    "@typescript-eslint/eslint-plugin": "^5.11.0",
    "@typescript-eslint/parser": "^5.11.0",
    "eslint": "^8.9.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "pkg": "^5.5.2",
    "prettier": "^2.5.1",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "@textile/tableland": "^0.0.8-dev7",
    "ethers": "^5.5.4",
    "node-fetch": "^2.6.7",
    "yargs": "^17.3.1"
  }
}